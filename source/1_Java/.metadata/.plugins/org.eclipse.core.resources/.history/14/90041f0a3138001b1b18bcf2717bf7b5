package com.lec.ex02_super;

public class Superij {
	private int i;
	private int j;
	public Superij() {}
	public Superij(int i, int j) {
		this.i = i;
		this.j = j;
	} //source-generate field
	
	//source- getter&setter
	public int getI() {
		return i;
	}
	public void setI(int i) {
		this.i = i;
	}
	public int getJ() {
		return j;
	}
	public void setJ(int j) {
		this.j = j;
	}
		
}
