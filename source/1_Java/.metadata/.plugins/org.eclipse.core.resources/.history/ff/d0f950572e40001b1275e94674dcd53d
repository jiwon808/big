package com.lec.ex3_set;
//이름, 학년  new Student("홍길동",5);
public class Student {
	private String name; //set에 student type의 객체를 넣어
	private int grade;
	public Student(String name, int grade) {
		this.name = name;
		this.grade = grade;
	}
	@Override
	public String toString() {
		return name+":"+grade; //"홍길동:5"
	}
	
	
}
