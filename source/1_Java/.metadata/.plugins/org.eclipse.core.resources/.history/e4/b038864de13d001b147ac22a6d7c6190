package com.lec.ex2_date;
/*생성자:
 * s1 = new Sawon ("a01","홍","IT")
 * s2= new Sawon("a01","홍","IT",2020,12,14)
 * System.out.println(s) => [사번]a01 [이름]홍 [입사일]20-12-14
 * 
 */
public class Ex09_Sawon {
		private int number;
		private String name;
		private String part;
		public Ex09_Sawon(int number, String name, String part) {
			this.number = number;
			this.name = name;
			this.part = part;
		}
		public int getNumber() {
			return number;
		}
		public String getName() {
			return name;
		}
		public String getPart() {
			return part;
		}
		
		//simple date format 
		//toString overide 
}
