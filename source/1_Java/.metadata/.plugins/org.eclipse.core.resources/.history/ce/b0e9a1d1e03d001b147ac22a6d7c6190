		SimpleDateFormat sdf = new SimpleDateFormat("yy-MM-dd(E) HH:mm");
		GregorianCalendar enterDate= new GregorianCalendar();
		System.out.println(sdf.format(date));
		System.out.println(sdf.format(date));
	}
}
