package com.lec.ex08Employee;

public abstract class Employee { //ë¬¸ì˜
	private String name;
	public Employee(String name) {
		this.name = name;
	}
	public abstract int computePay();
	private final int computeIncentive() {
		int pay = computePay();
		if(pay>2000000) {
			return (int)(pay*0.1);
		}
		return 0;
	}
}
