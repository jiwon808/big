				new Book("890ㄱ-100","Psychology","김지원");
	try {
		book.checkOut("kim");
		book1.checkout("lee");
		book.setCheckOutDate(new Date(
			new GregorianCalendar(2020, 10, 30).getTimeInMills()));//대출일조작
