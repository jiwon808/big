package com.lec.loop;

public class Gugudan_while {
		
		public static void main(String[] args) {
			int i = 1;
			int j = 3;    //초기값을 지정 해 줍니다 i값이 단부분, j값이 1~9까지 계산 되어지는 부분입니다
			while(i<=3) {
				while(j<=3)
				
				{
					
					System.out.println(i + " * " + j + " = " + i * j);
					j++;
				    //초기화나 증가값을 주지 않으면 무한 반복하기 때문에 꼭 넣어 주어야 합니다.
		              //j를 1씩 증가시켜주는 연산자입니다
				}

				
			}
		}

}
