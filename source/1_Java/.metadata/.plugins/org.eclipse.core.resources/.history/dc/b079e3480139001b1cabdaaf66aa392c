package com.lec.ex12_account;

public class CheckingAccount extends Account {
	private String cardNo;
	public CheckingAccount(String accountNo, String ownerName,)
	public void pay (String accountNo, String ownerName, int amount) {   //method만들기 . deposit&withdraw
		super(accountNo, ownerName, amount); //constructor에서만 쓸 수 있음 , this를 써야 null값으로 안들어가
	} //String,String,int,String
	new CheckingAccount(“111-1111”,”김길동”,[1000,] ”1234-1234-1234-1234”);

	ca.
	public void pay(String cardNo)

// 내 계좌잔액과 비교. 작액 만원. 물건가격5000원 
//string비교  . account의 deposit int amount가 checkingaccount int amount 보다 커야 돈이 빠져나가 
